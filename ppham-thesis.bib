journal = {Physical Review A},
month = feb,
number = {2},
pages = {022328},
title = {{Low-overhead constructions for the fault-tolerant Toffoli gate}},
url = {http://arxiv.org/abs/1212.5069},
volume = {87},
year = {2013}
}

