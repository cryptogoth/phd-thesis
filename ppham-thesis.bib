@article{Green2001,
abstract = {We propose definitions of \$\backslash QAC\^{}0\$, the quantum analog of the classical class \$\backslash AC\^{}0\$ of constant-depth circuits with AND and OR gates of arbitrary fan-in, and \$\backslash QACC[q]\$, the analog of the class \$\backslash ACC[q]\$ where \$\backslash Mod\_q\$ gates are also allowed. We prove that parity or fanout allows us to construct quantum \$\backslash MOD\_q\$ gates in constant depth for any \$q\$, so \$\backslash QACC[2] = \backslash QACC\$. More generally, we show that for any \$q,p > 1\$, \$\backslash MOD\_q\$ is equivalent to \$\backslash MOD\_p\$ (up to constant depth). This implies that \$\backslash QAC\^{}0\$ with unbounded fanout gates, denoted \$\backslash QACwf\^{}0\$, is the same as \$\backslash QACC[q]\$ and \$\backslash QACC\$ for all \$q\$. Since \$\backslash ACC[p] \backslash ne \backslash ACC[q]\$ whenever \$p\$ and \$q\$ are distinct primes, \$\backslash QACC[q]\$ is strictly more powerful than its classical counterpart, as is \$\backslash QAC\^{}0\$ when fanout is allowed. This adds to the growing list of quantum complexity classes which are provably more powerful than their classical counterparts. We also develop techniques for proving upper bounds for \$\backslash QACC\^{}0\$ in terms of related language classes. We define classes of languages \$\backslash EQACC\$, \$\backslash NQACC\$ and \$\backslash BQACC\_\{\backslash rats\}\$. We define a notion of \$\backslash log\$-planar \$\backslash QACC\$ operators and show the appropriately restricted versions of \$\backslash EQACC\$ and \$\backslash NQACC\$ are contained in \$\backslash P/\backslash poly\$. We also define a notion of \$\backslash log\$-gate restricted \$\backslash QACC\$ operators and show the appropriately restricted versions of \$\backslash EQACC\$ and \$\backslash NQACC\$ are contained in \$\backslash TC\^{}0\$.},
archivePrefix = {arXiv},
arxivId = {quant-ph/0106017},
author = {Green, Frederic and Homer, Steven and Moore, Cristopher and Pollett, Christopher},
eprint = {0106017},
file = {:Users/ppham/Dropbox/arXiv/0106017v1.pdf:pdf},
month = jun,
primaryClass = {quant-ph},
title = {{Counting, Fanout, and the Complexity of Quantum ACC}},
